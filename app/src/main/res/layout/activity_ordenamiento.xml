<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BC92FA"
    tools:context=".Ordenamiento.OrdenamientoActivity">

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="413dp"
        android:layout_height="302dp"
        android:visibility="visible"

        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5"
        tools:layout_editor_absoluteX="-2dp"
        tools:ignore="MissingConstraints">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/bt1"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />

            <Button
                android:id="@+id/bt2"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />

            <Button
                android:id="@+id/bt3"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/bt4"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />

            <Button
                android:id="@+id/bt5"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />

            <Button
                android:id="@+id/bt6"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/bt7"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />

            <Button
                android:id="@+id/bt8"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp"
                android:visibility="visible" />

            <Button
                android:id="@+id/bt9"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/bt10"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />

            <Button
                android:id="@+id/bt11"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />

            <Button
                android:id="@+id/bt12"
                android:layout_width="23dp"
                android:layout_height="69dp"
                android:layout_weight="1"
                android:backgroundTint="#EDCF62"
                android:fontFamily="@font/triogb"
                android:textColor="#000000"
                android:textSize="34sp" />
        </TableRow>


    </TableLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="410dp"
        android:layout_height="77dp"
        android:visibility="visible"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="1dp"
        tools:ignore="MissingConstraints">

        <Button
            android:id="@+id/btValidar"
            android:layout_width="205dp"
            android:layout_height="match_parent"
            android:backgroundTint="#ED6262"
            android:fontFamily="@font/triogb"
            android:text="Validar"
            android:textColor="#000000"
            android:textSize="34sp" />

        <Button
            android:id="@+id/btCompletar"
            android:layout_width="205dp"
            android:layout_height="match_parent"
            android:backgroundTint="#ED6262"
            android:fontFamily="@font/triogb"
            android:text="COMPLETAR"
            android:textColor="#000000"
            android:textSize="34sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textopuntaje"
        android:layout_width="78dp"
        android:layout_height="67dp"
        android:text=""
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:layout_width="406dp"
        android:layout_height="68dp"
        android:orientation="horizontal"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:id="@+id/crono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="00:00:000"
            android:textSize="45dp"
            android:textColor="@color/black" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="414dp"
        android:layout_height="64dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="65dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/texto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:fontFamily="@font/triogb"
            android:textColor="#000000"
            android:textSize="35dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>